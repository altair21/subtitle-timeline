{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {},
      "babel-module": {}
    },
    "react": {
      "version": "detect"
    }
  },
  "plugins": [
  ],
  "rules": {
    "no-continue": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/href-no-hash": [0],
    "default-case": 0,
    "consistent-return": 0,
    "max-len": [2, 260, 80],
    "import/prefer-default-export": 0,
    "prefer-destructuring": 0,
    "no-unused-vars": [1, { "argsIgnorePattern": "^_" }],
    "react/require-extension": 0,
    "object-shorthand": 0,
    "no-plusplus": 0,
    "arrow-parens": 0,
    "semi": 1
  }
}